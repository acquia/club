{
    "alias": "club.phar",
    "chmod": "0755",
    "directories": [
        "src",
        "Resources"
    ],
    "files": [
        "bin/application.php",
        "vendor/autoload.php"
    ],
    "finder": [
        {
            "name": "/(\\.php|\\.pem|\\.json|\\.yml|\\.twig)$/",
            "exclude": ["Tests","tests", "docs"],
            "in": "vendor"
        }
    ],
    "compactors": [
        "Herrera\\Box\\Compactor\\Php"
    ],
    "compression": "GZ",
    "main": "bin/club",
    "output": "club.phar",
    "git-version": "package_version",
    "stub": true
}
